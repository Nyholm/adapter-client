{
    "name": "php-http/adapter-core",
    "description": "HTTP Adapter default implementations and internal classes",
    "license": "MIT",
    "keywords": ["http", "adapter"],
    "homepage": "http://php-http.org",
    "authors": [
        {
            "name": "Eric GELOEN",
            "email": "geloen.eric@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.4",
        "php-http/adapter": "dev-master",
        "php-http/adapter-helper": "dev-master",
        "phly/http": "^0.12.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.4"
    },
    "autoload": {
        "psr-4": {
            "Http\\Adapter\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Http\\Adapter\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.1-dev"
        }
    },
    "prefer-stable": true,
    "minimum-stability": "dev"
}
